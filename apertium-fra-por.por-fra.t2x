    <!--
    This rule should disambiguate identical 1ps and 3ps verb forms. But
    everything involving <clip pos="2" part="pers"/> seems to fail. Why?
    In any case, I have verified that the chunks do indeed get here (by
    duplicating one of the clips in the <out> section), and I have verified
    that is indeed <clip pos="2" part="pers"/> that causes the problem (by
    taking all mentions of it out and trying various combinations). Tests
    involving it fail, and <let>s using it have no effect. (See the
    outdented <let> below.)
    -->
    <rule comment="REGLA: SN SV">
      <pattern>
        <pattern-item n="SN"/>
        <pattern-item n="SV"/>
      </pattern>
      <action>
        <let>
          <var n="subj_vist"/>
          <lit v="yes"/>
        </let>
        <let>
          <clip pos="2" part="pers"/>
          <lit-tag v="p3"/>
        </let>
        <call-macro n="assigna_gen_nbr">
          <with-param pos="1"/>
        </call-macro>
        <call-macro n="assigna_nombre_subj">
          <with-param pos="1"/>
        </call-macro>
        <let>
          <clip pos="2" part="pers"/>
          <var n="pers_subj"/>
        </let>
        <choose>
          <when>
            <test>
              <and>
                <!--
                should have separate testing for pii, since 1ps and 3ps are
                only the same for certain irregular verbs
                -->
                <not>
                  <equal>
                    <clip pos="2" part="eq_1ps_3ps"/>
                    <lit-tag v=""/>
                  </equal>
                </not>
                <equal>
                  <clip pos="1" part="nbr"/>
                  <lit-tag v="sg"/>
                </equal>
                <equal>
                  <clip pos="2" part="nbr"/>
                  <lit-tag v="sg"/>
                </equal>
                <or>
                  <and>
                    <equal>
                      <var n="pers_subj"/>
                      <lit v="p1"/>
                    </equal>
                    <equal>
                      <clip pos="2" part="pers"/>
                      <lit v="p3"/>
                    </equal>
                  </and>
                  <and>
                    <equal>
                      <var n="pers_subj"/>
                      <lit v="p3"/>
                    </equal>
                    <equal>
                      <clip pos="2" part="pers"/>
                      <lit v="p1"/>
                    </equal>
                  </and>
                </or>
              </and>
            </test>
            <let>
              <clip pos="2" part="pers"/>
              <var n="pers_subj"/>
            </let>
          </when>
        </choose>
        <out>
          <chunk>
            <clip pos="1" part="lem"/>
            <clip pos="1" part="tags"/>
            <clip pos="1" part="chcontent"/>
          </chunk>
          <b pos="1"/>
          <chunk>
            <clip pos="2" part="lem"/>
            <clip pos="2" part="tags"/>
            <clip pos="2" part="chcontent"/>
          </chunk>
        </out>
      </action>
    </rule>
